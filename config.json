{
  "host": "",
  "urls": [
    "/index.html",
    "/404.html"
  ],
  "copy": [],
  "seo": {
    "title": {
        "max": 60,
        "min": 20
    },
    "description": {
        "max": 200,
        "min": 150
    },
    "h1": {
        "max": 30,
        "min": 1
    }
  },
  "accessibility": {
    "aria": true,
    "focus": true,
    "lang": true,
    "outline": true,
    "tabindex": true
  },
  "img": {
    "lazy": true,
    "alt": true,
    "title": true,
    "webp": true,
    "responsive": true
  },
  "html": {
    "minify": false
  },
  "css": {
    "async": true,
    "purge": {
      "safeClasses": ["/^owl-/", "sticky-navbar", "/^slicknav/", "show", "b24-form-sign-abuse-info", "b24-form-sign-info"],
      "blockClasses": []
    },
    "minify": true,
    "critical": true,
    "combine": true
  },
  "js": {
    "async": true,
    "purge": false,
    "minify": true,
    "combine": false
  },
  "link": {
    "relativeUrl": false,
    "relativeAsset": false,
    "removeExtension": [".html", ".htm"],
    "removeIndex": true
  },
  "attrAssetMap": ["data-image-src", "data-src"],
  "chromePath": null,
  "log": {
    "verbose": false
  },
  "debug": true
}